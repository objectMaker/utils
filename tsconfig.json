{
	"compilerOptions": {
	  "module": "commonjs",
	  "noImplicitAny": true,
	  "removeComments": true,
	  "preserveConstEnums": true,
	  "sourceMap": true,
	  "target": "ES5",
	  "outDir": "./build",
	  "moduleResolution":"Node",
	  "esModuleInterop":true,
	  "baseUrl": ".",
	  "paths": {
		"@/*": ["src/*"]
	  }
	},
	"include": ["src/**/*","bin/**/*"],
  }